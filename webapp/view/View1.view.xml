<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:commons="sap.suite.ui.commons" controllerName="ovly.holidays.controller.View1" displayBlock="true">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}" busy="false" >
					<content>
					   <form:SimpleForm layout="ResponsiveGridLayout" adjustLabelSpan="true" editable="true" labelSpanM="3" emptySpanL="3" emptySpanM="3">
					   	<form:content>
					   		<Label text="Country"/>
					   		<Select id="select" forceSelection="false" busyIndicatorDelay="0">
					   			<items>

					   			</items>
					   		</Select>
					   		<Label text="Year"/>
					   		<DatePicker id="datepicker" placeholder="Year" displayFormat="yyyy"/>
					   		<Label text=""/>
					   		<Button text="Search" press="onSearch" icon="sap-icon://search"/>
					   	</form:content>
					   </form:SimpleForm>
					   <commons:Timeline id="timeline" showHeaderBar="false" alignment="Left">
							<commons:content>
								
							</commons:content>
								
					   </commons:Timeline>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>